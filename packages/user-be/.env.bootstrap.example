# Bootstrap credentials for AWS Secrets Manager
# Copy this file to .env.bootstrap and fill in your credentials
# These are the ONLY credentials needed to fetch all other secrets from AWS

AWS_REGION=ap-south-2

# AWS Secrets Manager credentials (get these from your AWS account)
SECRETS_AWS_ACCESS_KEY_ID=your_secrets_access_key_id
SECRETS_AWS_SECRET_ACCESS_KEY=your_secrets_secret_access_key
SECRET_NAME_AWS_USER_BE=sih-swaraj-user-be
